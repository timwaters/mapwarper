    <li class="responsive responsive_narrow"><%= link_to_unless_current(t('.help'), help_path) { "<span>#{t('.help')}</span>".html_safe } %></li>
    <li class=""><%= link_to_unless_current("Privacy", privacy_path) { "<span>Privacy</span>".html_safe } %></li>
    <li class=""><%= link_to_unless_current("Find Historic Maps by Category", by_category_path) { "<span>Find Historic Maps by Category</span>".html_safe } %></li>
    
  </ul>
</div>
