<div id="nav">
  <ul id="navlist" class="<%= I18n.locale %>">
    <li class="responsive"><%= link_to_unless_current(t('.home'), root_path) { "<span>#{t('.home')}</span>".html_safe } %></li>
    <li class=""><%= link_to_unless((current_page?(maps_url) && !current_page?(maps_url(:show_warped =>1))),t('.maps'), maps_url) { "<span>#{t('.maps')}</span>".html_safe } %></li>
    <li class="responsive"><%= link_to_unless_current(t('.geosearch_maps'), geosearch_maps_url(:show_warped =>1)) { "<span>#{t('.geosearch_maps')}</span>".html_safe } %></li>  
